{
  "name": "@clawpwa/pwa-channel",
  "version": "0.1.0",
  "description": "PWA channel plugin for OpenClaw Gateway",
  "type": "module",
  "main": "index.ts",
  "types": "index.d.ts",
  "files": [
    "index.ts",
    "src",
    "clawdbot.plugin.json"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "openclaw",
    "pwa",
    "gateway",
    "plugin",
    "channel"
  ],
  "author": "",
  "license": "MIT",
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "pwa",
      "label": "PWA",
      "selectionLabel": "PWA (Progressive Web App)",
      "docsPath": "/channels/pwa",
      "blurb": "PWA client channel for OpenClaw",
      "order": 100
    },
    "install": {
      "npmSpec": "@clawpwa/pwa-channel",
      "localPath": ".",
      "defaultChoice": "local"
    }
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "clawdbot": "^2026.1.24-2",
    "typescript": "^5.6.0"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
